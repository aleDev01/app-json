{
    "name": "MANAGER Music",
    "logo": "https://telegra.ph/file/7495e689ab5d95e215afd.jpg",
    "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat..",
    "keywords": ["music", "voicechat", "telegram"],
    "repository": "https://github.com/PsychoBots/VCMusical",
    "stack": "container",
    "env": {
        "SESSION_NAME": {
            "description": "Session string (pyrogram)",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "From @BotFather",
            "required": true
        },
        "BOT_NAME": {
            "description": "Your VC Play by Matrix VC Bot Name.",
            "required": false,
            "value": ""
       },
        "API_ID": {
            "description": "From my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "From my.telegram.org/apps",
            "required": true
        },
        "SUDO_USERS": {
            "description": "Full-access users, counted as admin everywhere, seperated by space.",
            "required": true
        },
        "DURATION_LIMIT": {
            "description": "In minutes!",
            "required": true,
            "value": "25"
        }
    },
    "buildpacks": [
       {
      "url": "heroku/python"
       }
     ]
  }
